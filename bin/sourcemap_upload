#!/bin/bash
curl -o "./index.ios.map" "http://localhost:8081/index.ios.map?platform=ios&dev=true&minify=false"
curl -o "./index.ios.bundle" "http://localhost:8081/index.ios.bundle?platform=ios&dev=true&minify=false"
sentry-cli releases new $1
sentry-cli releases files $1 upload-sourcemaps . --url-prefix="/" --rewrite
rm index.ios.*